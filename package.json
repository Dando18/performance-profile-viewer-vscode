{
  "name": "profileviewer",
  "displayName": "ProfileViewer",
  "description": "View profiles of source code.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.79.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "profileviewer.viewHPCToolkitProfile",
        "title": "View HPCToolkit Profile",
        "category": "Profile"
      },
      {
        "command": "profileviewer.viewCaliperProfile",
        "title": "View Caliper Profile",
        "category": "Profile"
      },
      {
        "command": "profileviewer.viewTAUProfile",
        "title": "View TAU Profile",
        "category": "Profile"
      },
      {
        "command": "profileviewer.viewPyInstrumentProfile",
        "title": "View PyInstrument Profile",
        "category": "Profile"
      },
      }
    ],
    "viewsContainers": {
      "panel": [
        {
          "id": "profile-panel",
          "title": "Profile Panel",
          "icon": "media/profile.svg"
        }
      ]
    },
    "views": {
      "profile-panel": [
        {
          "id": "profileViewer",
          "name": "Profile Viewer"
        }
      ]
    },
    "colors": [
      {
        "id": "profileViewer.speed0",
        "description": "The color used to highlight functions.",
        "defaults": {
          "light": "#ff0000",
          "dark": "#ff0000"
        }
      },
      {
        "id": "profileViewer.speed1",
        "description": "The color used to highlight functions.",
        "defaults": {
          "light": "#ff3300",
          "dark": "#ff3300"
        }
      },
      {
        "id": "profileViewer.speed2",
        "description": "The color used to highlight functions.",
        "defaults": {
          "light": "#ff6600",
          "dark": "#ff6600"
        }
      },
      {
        "id": "profileViewer.speed3",
        "description": "The color used to highlight functions.",
        "defaults": {
          "light": "#ff9900",
          "dark": "#ff9900"
        }
      },
      {
        "id": "profileViewer.speed4",
        "description": "The color used to highlight functions.",
        "defaults": {
          "light": "#ffcc00",
          "dark": "#ffcc00"
        }
      },
      {
        "id": "profileViewer.speed5",
        "description": "The color used to highlight functions.",
        "defaults": {
          "light": "#ffff00",
          "dark": "#ffff00"
        }
      },
      {
        "id": "profileViewer.speed6",
        "description": "The color used to highlight functions.",
        "defaults": {
          "light": "#ccff00",
          "dark": "#ccff00"
        }
      },
      {
        "id": "profileViewer.speed7",
        "description": "The color used to highlight functions.",
        "defaults": {
          "light": "#99ff00",
          "dark": "#99ff00"
        }
      },
      {
        "id": "profileViewer.speed8",
        "description": "The color used to highlight functions.",
        "defaults": {
          "light": "#66ff00",
          "dark": "#66ff00"
        }
      },
      {
        "id": "profileViewer.speed9",
        "description": "The color used to highlight functions.",
        "defaults": {
          "light": "#33ff00",
          "dark": "#33ff00"
        }
      },
      {
        "id": "profileViewer.speed10",
        "description": "The color used to highlight functions.",
        "defaults": {
          "light": "#00ff00",
          "dark": "#00ff00"
        }
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.79.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "@vscode/test-electron": "^2.3.2"
  }
}
