{
  "name": "profileviewer",
  "displayName": "ProfileViewer",
  "description": "View profiles of source code.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.79.0"
  },
  "categories": [
    "Profile"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "profileviewer.openHPCToolkitProfile",
        "title": "Open HPCToolkit Profile",
        "category": "Profile"
      },
      {
        "command": "profileviewer.openCaliperProfile",
        "title": "Open Caliper Profile",
        "category": "Profile"
      },
      {
        "command": "profileviewer.openTAUProfile",
        "title": "Open TAU Profile",
        "category": "Profile"
      },
      {
        "command": "profileviewer.openPyInstrumentProfile",
        "title": "Open PyInstrument Profile",
        "category": "Profile"
      },
      {
        "command": "profileviewer.openProfile",
        "title": "Open Profile",
        "category": "Profile",
        "icon": "$(add)"
      },
      {
        "command": "profileviewer.openFlameGraph",
        "title": "Open Flame Graph",
        "category": "Profile",
        "icon": "$(graph)"
      }
    ],
    "customEditors": [
      {
        "viewType": "profileviewer.profileTreeEditor",
        "displayName": "Profile Tree",
        "priority": "option",
        "icon": "$(circuit-board)",
        "selector": [
          {
            "filenamePattern": "*"
          }
        ]
      },
      {
        "viewType": "profileviewer.profileFlameGraphViewer",
        "displayName": "Profile Flame Graph",
        "priority": "option",
        "icon": "$(flame)",
        "selector": [
          {
            "filenamePattern": "*"
          }
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.79.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/codicons": "0.0.20",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "@vscode/test-electron": "^2.3.2"
  }
}
